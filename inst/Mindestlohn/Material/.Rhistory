dat_seq = read_dta('seq98.dta')
#Wichtige Spalten herausfiltern
dat_seq = select(dat_seq, serial, serno, a3, a4, a5, c1_, d1, d2, d5, d11)
#Spalten umbennen
dat_seq = rename(dat_seq,
hours = a3,
overtime = a4,
tu = c1_,
male = d1,
age = d2,
educ = d5)
#Neue Spalten
dat_seq = mutate(dat_seq, "union" =
case_when(tu < 7 ~ 1))
dat_seq$male = replace(dat_seq$male, dat_seq$male == 2, 0)
aa_ = dat_seq %>%
group_by(age) %>%
summary(age)
dat_seq = mutate(dat_seq, "aa_" = case_when(
age > 0 ~ age))
dat_seq = mutate(dat_seq, "ee_" = case_when(
educ < 7 ~ educ))
#Intervalle erstellen
dat_seq = mutate(dat_seq,
"w1" = case_when(
d11 == 1 ~ 0,
d11 == 2 ~ 51,
d11 == 3 ~ 81,
d11 == 4 ~ 141,
d11 == 5 ~ 181,
d11 == 6 ~ 221,
d11 == 7 ~ 261,
d11 == 8 ~ 311,
d11 == 9 ~ 361,
d11 == 10 ~ 431,
d11 == 11 ~ 541,
d11 == 12 ~ 681,
))
dat_seq = mutate(dat_seq,
"w2" = case_when(
d11 == 1 ~ 50,
d11 == 2 ~ 80,
d11 == 3 ~ 140,
d11 == 4 ~ 180,
d11 == 5 ~ 220,
d11 == 6 ~ 260,
d11 == 7 ~ 310,
d11 == 8 ~ 360,
d11 == 9 ~ 430,
d11 == 10 ~ 540,
d11 == 11 ~ 680,
d11 == 12 ~ 800,
))
dat_seq = mutate(dat_seq,
"lw1" = log(w1),
"lw2" = log(w2),
"lh" = log(hours))
#Intervalle mit Survival
library(survival)
